<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ListarDicaFrAcoesDica"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/borda_linear_layout"
    android:padding="16dp">


    <TextView
        android:id="@+id/ListarDicaFrAcoesDicaTextViewVisualizarDica"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/gtc_visualizar_dica"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:padding="20dp"
        android:clickable="true"
        android:focusable="true"/>


    <TextView
        android:id="@+id/ListarDicaFrAcoesDicaTextViewExcluirDica"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/gtc_excluir_dica"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:padding="20dp"
        android:clickable="true"
        android:focusable="true"/>

</LinearLayout>